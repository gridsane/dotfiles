Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintlight
Xft.rgba: rgb
Xcursor: DMZ

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Meta-k: perl:pageup
URxvt.keysym.Meta-j: perl:pagedown
URxvt.iso14755: False
URxvt.perl-ext-common: default,tabbed,clipboard,scroll
URxvt.font: xft:Inconsolata LGC-8
URxvt.letterSpace: -1
URxvt.scrollstyle: plain
URxvt.pointerBlank: true
URxvt.pointerBlankDelay: 5

*background: #272822
*foreground: #F8F8F2

! Black
*color0:   #111111
*color8:   #666666

! Red
*color1:   rgb:d3/62/65
*color9:   rgb:ef/81/71

! Green
*color2:   #aece91
*color10:  #cfefb3

! Yellow
*color3:   rgb:e7/e1/8c
*color11:  rgb:ff/f7/96

! Blue
*color4:   #5297cf
*color12:  #74b8ef

! Magenta
*color5:   #963c59
*color13:  #b85e7b

! Cyan
*color6:   #5E7175
*color14:  #A3BABF

! White
*color7:   #bebebe
*color15:  #ffffff

! vim: et sw=2 syn=xdefaults
